
<div id="search-page">
       
    <div class="to-right">
        <div class="search-area">
                <input id="looking" type="text" placeholder="BUSCAR" class="search" />
        </div>
    </div>
</div>
                    

<script type="text/javascript">
    var cache = [];
    $(document).ready(function(){
        $( "#looking" ).autocomplete({
            delay: 500,
            source: function( request, response ){
                $.ajax('/livesearch',{
                    method: 'POST',
                    data: { q: $('#looking').val() },
                    success: function(result){
                        response(result.suggestions);
                        console.log(result);
                    }
                });
            }            
        });
    });
